{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "IMF Lending",
    "subtitle": "Loans Dispersed",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/JaydenLaw87/JaydenLaw87.github.io/main/data_IMF_creditOutstanding.csv"
  },
  "height": 300,
  "width": "container",
  "mark": {"type": "bar", "color": "#0633c5ee"},
  "encoding": {
    "x": {
      "field": "Years",
      "type": "temporal",
      "axis": {
        "title": "Years",
        "grid": false,
        "ticks": false,
        "labelAngle": 45
      }
    },
    "y": {
      "field": "Billions",
      "type": "quantitative",
      "title": "Billions $",
      "axis": {"grid": false}
    }
  }
}
